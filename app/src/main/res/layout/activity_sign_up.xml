<?xml version="1.0" encoding="utf-8"?><RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"    xmlns:tools="http://schemas.android.com/tools"    android:layout_width="match_parent"    android:layout_height="match_parent"    android:background="@color/colorPrimaryDark"    tools:context=".ui.sing_up.SignUpActivity">    <ImageView        android:layout_marginTop="@dimen/_38sdp"        android:layout_width="90dp"        android:layout_height="90dp"        android:layout_centerHorizontal="true"        android:layout_gravity="center_horizontal"        android:layout_marginBottom="@dimen/spacing_mlarge"        android:src="@drawable/shopoapp"        android:tint="@android:color/white" />    <LinearLayout        android:layout_width="match_parent"        android:layout_height="wrap_content"        android:layout_centerInParent="true"        android:layout_centerHorizontal="true"        android:layout_marginStart="@dimen/dimen_24"        android:layout_marginEnd="@dimen/dimen_24"        android:orientation="vertical">        <EditText            android:fontFamily="@font/cour"            android:id="@+id/ID_Name"            android:layout_width="match_parent"            android:layout_height="@dimen/_38sdp"            android:layout_marginTop="@dimen/dimen_34"            android:paddingEnd="@dimen/dimen_8"            android:background="@drawable/edit_text_round_bg_overlay_white"            android:hint="ادخل اسمك"            android:layout_gravity="center"            android:gravity="center"            android:inputType="text"            android:textColorHint="@color/colorPrimaryDark"            android:textColor="@color/colorPrimaryDark"            android:paddingStart="@dimen/dimen_8" />        <EditText            android:fontFamily="@font/cour"            android:paddingEnd="@dimen/dimen_8"            android:gravity="center"            android:textColorHint="@color/colorPrimaryDark"            android:textColor="@color/colorPrimaryDark"            android:id="@+id/ID_Email"            android:layout_width="match_parent"            android:layout_height="@dimen/_38sdp"            android:layout_marginTop="@dimen/dimen_24"            android:background="@drawable/edit_text_round_bg_overlay_white"            android:hint="ادخل البريد الالكتروني"            android:inputType="textEmailAddress"            android:paddingStart="@dimen/dimen_8" />        <EditText            android:fontFamily="@font/cour"            android:gravity="center"            android:paddingEnd="@dimen/dimen_8"            android:id="@+id/ID_Phone"            android:layout_width="match_parent"            android:layout_height="@dimen/_38sdp"            android:layout_marginTop="@dimen/dimen_24"            android:background="@drawable/edit_text_round_bg_overlay_white"            android:hint="ادخل رقم الموبايل"            android:inputType="phone"            android:textColorHint="@color/colorPrimaryDark"            android:textColor="@color/colorPrimaryDark"            android:paddingStart="@dimen/dimen_8" />    </LinearLayout>    <RelativeLayout        android:layout_width="match_parent"        android:layout_height="match_parent"        android:layout_marginTop="@dimen/dimen_8">        <Button            android:textColor="@color/white"            android:fontFamily="@font/cour"            android:id="@+id/btn_Register"            android:layout_width="match_parent"            android:layout_height="@dimen/_38sdp"            android:layout_alignParentBottom="true"            android:layout_marginStart="@dimen/dimen_24"            android:layout_marginTop="@dimen/dimen_8"            android:layout_marginEnd="@dimen/dimen_24"            android:layout_marginBottom="@dimen/dimen_24"            android:text=" أكمل البيانات"            android:background="@drawable/back_btn"            android:textStyle="bold" />    </RelativeLayout></RelativeLayout>