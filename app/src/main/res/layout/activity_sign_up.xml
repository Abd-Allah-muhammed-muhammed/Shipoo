<?xml version="1.0" encoding="utf-8"?><RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"    xmlns:app="http://schemas.android.com/apk/res-auto"    xmlns:tools="http://schemas.android.com/tools"    android:layout_width="match_parent"    android:layout_height="match_parent"    tools:context=".ui.sing_up.SignUpActivity">    <RelativeLayout        android:id="@+id/layout1"        android:layout_width="match_parent"        android:layout_height="188dp"        android:background="@drawable/curv">        <TextView            android:layout_marginTop="@dimen/dimen_42"            android:id="@+id/shippo"            android:layout_width="200dp"            android:layout_height="50dp"            android:layout_centerHorizontal="true"            android:background="@drawable/shopoapp"            android:gravity="center" />        <ImageView            android:src="@drawable/logo"            android:id="@+id/round"            android:layout_width="@dimen/_100"            android:layout_height="@dimen/_100"            android:layout_alignParentStart="true"            android:layout_alignParentBottom="true"            android:background="@drawable/round_sh" />    </RelativeLayout>    <LinearLayoutandroid:layout_below="@+id/layout1"        android:layout_width="match_parent"        android:layout_height="wrap_content"        android:layout_centerInParent="true"        android:layout_centerHorizontal="true"        android:layout_marginStart="@dimen/dimen_24"        android:layout_marginEnd="@dimen/dimen_24"        android:orientation="vertical">        <EditText            android:id="@+id/ID_Name"            android:layout_width="match_parent"            android:layout_height="@dimen/dimen_42"            android:layout_marginTop="@dimen/dimen_34"            android:background="@drawable/edittext_rounded_corners"            android:hint="@string/client_register_name_et"            android:inputType="text"            android:paddingStart="@dimen/dimen_8" />        <EditText            android:id="@+id/ID_Email"            android:layout_width="match_parent"            android:layout_height="@dimen/dimen_42"            android:layout_marginTop="@dimen/dimen_24"            android:background="@drawable/edittext_rounded_corners"            android:hint="@string/client_register_mail_et"            android:inputType="textEmailAddress"            android:paddingStart="@dimen/dimen_8" />        <EditText            android:id="@+id/ID_Phone"            android:layout_width="match_parent"            android:layout_height="@dimen/dimen_42"            android:layout_marginTop="@dimen/dimen_24"            android:background="@drawable/edittext_rounded_corners"            android:hint="@string/client_register_phone_et"            android:inputType="phone"            android:paddingStart="@dimen/dimen_8" />    </LinearLayout>    <RelativeLayout        android:layout_width="match_parent"        android:layout_height="match_parent"        android:layout_marginTop="@dimen/dimen_8">        <Button            android:id="@+id/btn_Register"            android:layout_width="match_parent"            android:layout_height="wrap_content"            android:layout_alignParentBottom="true"            android:layout_marginStart="@dimen/dimen_24"            android:layout_marginTop="@dimen/dimen_8"            android:layout_marginEnd="@dimen/dimen_24"            android:layout_marginBottom="@dimen/dimen_24"            android:background="@drawable/courn_edt"            android:text=" تسجيل الدخول"            android:textColor="@color/white"            android:textStyle="bold" />    </RelativeLayout></RelativeLayout>